#!/usr/bin/env bash
#
## Enables checking of all commands
set -e




echo "内核信息"
uname -a

echo "现存 node 版本 ==========================================="
echo node version: $(node --version)
echo npm version: $(npm --version)

# install node js package
#
npm install

#
# run gulp commands
#
NODE_ENV=production ./node_modules/gulp/bin/gulp.js preDeploy



echo "====> $0 EXECUTE OVER <===="
